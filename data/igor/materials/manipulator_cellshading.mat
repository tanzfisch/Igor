<?xml version="1.0"?>
<Igor>
	<!-- material definition -->
	<Material name="mica.manipulator.cellshading" uuid="6dbf625e-7b48-11ed-acf8-ab7ebd6463ff" visibility="Private">
		<States>
            <Wireframe>On</Wireframe>
            <CullFace>On</CullFace>
            <CullFaceFunc>Front</CullFaceFunc>
			<DepthTest>On</DepthTest>
            <DepthMask>Off</DepthMask>
            <Blend>Off</Blend>
		</States>
        <Program>
            <Vertex>
<![CDATA[#version 450

layout(location = 0) in vec3 in_position;

uniform mat4 igor_modelViewProjection;

void main()
{
	gl_Position = igor_modelViewProjection * vec4(in_position,1);
}]]>
            </Vertex>
            <Fragment>
<![CDATA[#version 450

layout(location = 0) out vec4 out_color;

void main()
{
	out_color = vec4(1,1,0,1);
}
]]>
            </Fragment>
        </Program>            
	</Material>
</Igor>
